    <resource path="/{connectionId}/notebooks/Dynamic/sections">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="POST" id="CreateSectionInNotebook">
        <doc title="Create section in a notebook&#xA;Create section in a notebook." />
        <request>
          <param style="query" name="notebookKey" type="xs:string" required="true">
            <doc title="A key identifying the notebook. Note this is specific to the API and cannot be hand-typed." />
          </param>
          <representation mediaType="application/json">
            <param style="plain" name="name" path="/name" type="xs:string" />
          </representation>
        </request>
        <response>
          <representation mediaType="application/json" element="service:CreateSectionInNotebookResponse" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/sections/Dynamic/pages">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="POST" id="CreatePageInSection">
        <doc title="Create page in a section&#xA;Create new page in a specified section." />
        <request>
          <param style="query" name="notebookKey" type="xs:string" required="true">
            <doc title="A key identifying the notebook. Note this is specific to the API and cannot be hand-typed." />
          </param>
          <param style="query" name="sectionId" type="xs:string" required="true">
            <doc title="The unique identifier of the section within the notebook." />
          </param>
          <representation mediaType="application/json">
            <param style="plain" name="pageContent" path="" type="xs:string" required="true">
              <doc title="Page content in HTML." />
            </param>
          </representation>
        </request>
        <response>
          <representation mediaType="application/json" element="service:Page" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/sections/Dynamic/pages">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="GET" id="GetPagesInSection">
        <doc title="Get pages for a specific section&#xA;Get pages for a specific section." />
        <request>
          <param style="query" name="notebookKey" type="xs:string" required="true">
            <doc title="A key identifying the notebook. Note this is specific to the API and cannot be hand-typed." />
          </param>
          <param style="query" name="sectionId" type="xs:string" required="true">
            <doc title="Unique Id of the section." />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:GetPagesInSectionResponse" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/pages">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="POST" id="CreatePageInQuickNotes">
        <doc title="Create a page in Quick Notes&#xA;Create a new page in the Quick Notes section." />
        <request>
          <representation mediaType="application/json">
            <param style="plain" name="pageContent" path="" type="xs:string" required="true">
              <doc title="Page content in HTML." />
            </param>
          </representation>
        </request>
        <response>
          <representation mediaType="application/json" element="service:Page" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/pages/Dynamic/content">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="GET" id="GetPageContent">
        <doc title="Get page content&#xA;Get HTML page content." />
        <request>
          <param style="query" name="notebookKey" type="xs:string" required="true">
            <doc title="A key identifying the notebook. Note this is specific to the API and cannot be hand-typed." />
          </param>
          <param style="query" name="sectionId" type="xs:string" required="true">
            <doc title="Unique Id of the section." />
          </param>
          <param style="query" name="pageId" type="xs:string" required="true">
            <doc title="Unique id of the page." />
          </param>
          <param style="query" name="preAuthenticated" type="xs:boolean" required="true" fixed="true" />
        </request>
        <response>
          <representation mediaType="application/json" element="xs:string" />
        </response>
      </method>
    </resource>
